<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/context
          http://www.springframework.org/schema/context/spring-context-3.0.xsd
          http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
          http://code.alibabatech.com/schema/dubbo 
		  http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <context:property-placeholder location="classpath:dubbo.properties"/>

    <dubbo:application name="${dubbo.application.name}"/>
    <dubbo:consumer check="false" timeout="15000"/>
    <dubbo:registry address="${dubbo.registry.address}"/>

    <!--以下配置为注册业务接口,所有控制器,业务层等用到的业务接口都需在此配置-->
    <dubbo:reference id="angleApiImpl" interface="com.bjike.goddess.organize.api.AngleAPI"/>
    <dubbo:reference id="arrangementApiImpl"
                     interface="com.bjike.goddess.organize.api.ArrangementAPI"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.DepartmentDetailAPI"
                     id="departmentDetailApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.DesignNumberInfoAPI"
                     id="designNumberInfoApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.DimensionAPI" id="dimensionApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.HierarchyAPI" id="hierarchyApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.InstructionClassifyAPI"
                     id="instructionClassifyApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.OperateAPI" id="operateApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.PositionDetailAPI"
                     id="positionDetailApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.PositionInstructionAPI"
                     id="positionInstructionApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.PositionInstructionsLogAPI"
                     id="positionInstructionsLogApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.PositionWorkDetailAPI"
                     id="positionWorkDetailApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.ReflectAPI" id="reflectApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.WorkRangeAPI" id="workRangeApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.ModuleTypeAPI" id="moduleTypeApiImpl"/>
    <dubbo:reference interface="com.bjike.goddess.organize.api.PositionDetailUserAPI" id="positionDetailUserApiImpl"/>


</beans>
